/* Confirm Dialog */

@import "colors.rasi"

* {
    background-color:       @bg;
    text-color:             @fg;
	/*font:				 	"spleen 14px";*/
    font:					"gameover 20px";
}

window {
    width:      			225px;
    padding:    			10px;
    border:					5px 5px 5px 5px;
    border-radius:          0px;
    border-color:           @bg2;
    location:               center;
    y-offset:               -2em;
}

entry {
    expand: 				true;
    text-color:				@fg;
}
